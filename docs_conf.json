{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "exclude": [ ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "_0_template": "node_modules/namis",
        "_1_template": "node_modules/ink-docstrap/template",
        "_2_template": "node_modules/tui-jsdoc-template",
        "_3_template": "node_modules/minami",
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs",
        "recurse": false
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "logo": {
            "url": "",
            "width": "0px",
            "height": "0px"
        },
        "footerText": "<b>Crabel Core Library for JavaScript</b>"
    }
}
